<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>RequestServlet</servlet-name>
        <servlet-class>com.testcy.servlet.RequestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RequestServlet</servlet-name>
        <url-pattern>/requestServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ParameterServlet</servlet-name>
        <servlet-class>com.testcy.servlet.ParameterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParameterServlet</servlet-name>
        <url-pattern>/parameterServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletDispather1</servlet-name>
        <servlet-class>com.testcy.servlet.ServletDispather1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDispather1</servlet-name>
        <url-pattern>/servletDispather1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletDispather2</servlet-name>
        <servlet-class>com.testcy.servlet.ServletDispather2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletDispather2</servlet-name>
        <url-pattern>/servletDispather2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ForwardServlet</servlet-name>
        <servlet-class>com.testcy.servlet.ForwardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ForwardServlet</servlet-name>
        <url-pattern>/forwardServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ResponseIOServlet</servlet-name>
        <servlet-class>com.testcy.servlet.ResponseIOServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseIOServlet</servlet-name>
        <url-pattern>/responseIOServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ResponseServlet1</servlet-name>
        <servlet-class>com.testcy.servlet.ResponseServlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseServlet1</servlet-name>
        <url-pattern>/responseServlet1</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ResponseServlet2</servlet-name>
        <servlet-class>com.testcy.servlet.ResponseServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseServlet2</servlet-name>
        <url-pattern>/responseServlet2</url-pattern>
    </servlet-mapping>
</web-app>